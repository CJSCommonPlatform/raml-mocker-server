#%RAML 0.8
title: TEST_Definition
version: v1
mediaType: application/json

/example:
  description: Response from the example
  get:
    responses:
      200:
        body:
          example: |
            {"test": "test"}

/faker:
  description: Autogenerated faker response
  get:
    responses:
      200:
        body:
          schema: !include schemas/faker.json

/schema-first:
  description: Has both schema first and then example
  get:
    responses:
      200:
        body:
          schema: !include schemas/faker.json
          example: |
            {"test": "test"}

/example-first:
  description: Has both example first and then schema
  get:
    responses:
      200:
        body:
          example: |
            {"test": "test"}
          schema: !include schemas/faker.json




